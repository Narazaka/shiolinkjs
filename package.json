{
  "name": "shiolinkjs",
  "version": "2.0.0",
  "description": "SHIOLINK interface for JavaScript",
  "homepage": "https://github.com/Narazaka/shiolinkjs",
  "main": "./dist/lib/shiolink.js",
  "types": "./dist/lib/shiolink.d.ts",
  "files": [
    "dist/lib"
  ],
  "scripts": {
    "dist": "rimraf dist && tsc",
    "test": "nyc mocha",
    "lint": "tslint -p .",
    "prepublish": "npm run dist && npm run test && npm run lint",
    "docdist": "cd doc && git clean -fd && git rm -rf * && cd .. && npm run doc && cd doc && touch .nojekyll && git add . && git commit -m doc && git push",
    "doc": "typedoc --mode file --out doc --readme Readme.md --name \"API Document\" --disableOutputCheck --excludeNotExported lib && cd doc && git status && cd .."
  },
  "dependencies": {
    "shiorijk": "^1.0.0"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.46",
    "@types/power-assert": "^1.4.29",
    "@types/sinon": "^4.1.2",
    "espower-typescript": "^8.1.2",
    "mocha": "^4.1.0",
    "nyc": "^11.4.1",
    "power-assert": "^1.4.4",
    "rimraf": "^2.6.2",
    "sinon": "^4.1.4",
    "tslint": "^5.9.1",
    "typedoc": "^0.9.0",
    "typescript": "~2.5.3"
  },
  "author": {
    "name": "narazaka",
    "url": "https://narazaka.net/"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Narazaka/shiolinkjs.git"
  }
}
